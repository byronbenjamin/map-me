<p id="notice"><%= notice %></p>

<h1>Listing Future Trips</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @future_trips.each do |future_trip| %>
      <tr>
        <td><%= link_to 'Show', future_trip %></td>
        <td><%= link_to 'Edit', edit_future_trip_path(future_trip) %></td>
        <td><%= link_to 'Destroy', future_trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Future trip', new_future_trip_path %>
